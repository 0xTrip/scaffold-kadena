# Set your deployer private key (required for deployment)
# Generate a new private key for testing: openssl rand -hex 32
export DEPLOYER_PRIVATE_KEY=your_private_key_here

# Set Chainweb environment (optional, defaults to "anvil")
export CHAINWEB=anvil

# For testnet verification (optional)
export BLOCKSCOUT_API_KEY=your_blockscout_api_key_here